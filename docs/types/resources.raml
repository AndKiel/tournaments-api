AccessToken:
  type: object
  properties:
    access_token: string
    created_at: integer,
    expires_in: integer
    refresh_token: string
    token_type: string
  example:
    access_token: fdf22ac62f7007c2a63291485446650e01637a986db59364e33761838e109ee4
    created_at: 1515571806
    expires_in: 7200
    refresh_token: 8a737a12ea6a3182aa9924b277d67d55c92f67fa3d03f5016aa50c787ea513b0
    token_type: bearer

AccessTokenInfo:
  type: object
  properties:
    application: Application
    created_at: integer
    expires_in_seconds: integer
    resource_owner_id: string
    scopes: string[]
  example:
    application:
      uid:
    created_at: 1515572788
    expires_in_seconds: 100
    resource_owner_id: 6a15b02f-0712-56dc-a290-915475d29abb
    scopes: ["public"]

Application:
  type: object
  properties:
    uid: nil

Competitor:
  type: object
  properties:
    id: string
    status:
      enum: ["enlisted", "confirmed"]

Player:
  type: object
  properties:
    id: string
    result_values: number[]
    table_number: integer

Round:
  type: object
  properties:
    competitors_limit: integer
    created_at: datetime
    id: string
    tables_count: integer

Tournament:
  type: object
  properties:
    competitors_limit: integer
    description:
      type: string
      required: false
    id: string
    name: string
    result_names: string[]
    starts_at: datetime
    status:
      enum: ["created", "in_progress", "ended"]

User:
  type: object
  properties:
    email: string
    id: string
    name: string
