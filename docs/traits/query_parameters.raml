filterable:
  usage: Apply this to any method that returns a <<typeName>> collection and allows filtering.
  queryParameters:
    filters:
      type: <<typeName>>Filters
      required: false

paginated:
  usage: Apply this to any method that returns paginated results.
  queryParameters:
    page:
      description: Specify the page that you want to retrieve, 25 results are returned per page.
      required: false
      type: integer
      default: 1
