{
  "$schema": "https://json-schema.org/draft-06/schema",
  "id": "file:/oauth/access_token_info.json#",
  "type": "object",
  "required": [
    "application",
    "created_at",
    "expires_in",
    "resource_owner_id",
    "scope"
  ],
  "properties": {
    "application": {
      "type": "object",
      "properties": {
        "uid": {
          "type": "null"
        }
      }
    },
    "created_at": {
      "type": "integer"
    },
    "expires_in": {
      "type": "integer"
    },
    "resource_owner_id": {
      "$ref": "file:/common/uuid.json#"
    },
    "scope": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    }
  }
}
