{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "file:/players.json",
  "type": "object",
  "required": [
    "players"
  ],
  "properties": {
    "players": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "competitor_id",
          "id",
          "result_values",
          "table_number"
        ],
        "properties": {
          "competitor_id": {
            "type": "string",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
          },
          "id": {
            "type": "string",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
          },
          "result_values": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "table_number": {
            "type": "integer"
          }
        }
      }
    }
  }
}
