{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "file:/round.json",
  "type": "object",
  "required": [
    "round"
  ],
  "properties": {
    "round": {
      "type": "object",
      "required": [
        "competitors_limit",
        "created_at",
        "id",
        "tables_count"
      ],
      "properties": {
        "competitors_limit": {
          "type": "integer"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "id": {
          "type": "string",
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
        },
        "tables_count": {
          "type": "integer"
        }
      }
    }
  }
}
