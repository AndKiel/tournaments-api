{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "file:/responses/competitor.json",
  "type": "object",
  "required": [
    "competitor"
  ],
  "properties": {
    "competitor": {
      "type": "object",
      "required": [
        "created_at",
        "id",
        "name",
        "status",
        "user_id"
      ],
      "properties": {
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "id": {
          "type": "string",
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
        },
        "name": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "user_id": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
        }
      }
    }
  }
}
